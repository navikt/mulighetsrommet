{
  "name": "@mr/arrangor-flate",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_MULIGHETSROMMET_API_MOCK='true' node ./server.js",
