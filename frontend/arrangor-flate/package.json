{
  "name": "@mr/arrangor-flate",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_MULIGHETSROMMET_API_MOCK='true' DISABLE_DEKORATOR='true' node ./server/main.js",
    "dev:vite": "react-router dev",
    "backend": "cross-env VITE_MULIGHETSROMMET_API_BASE='http://localhost:8080' node ./server/main.js",
    "start": "cross-env NODE_ENV=production VITE_MULIGHETSROMMET_API_BASE='http://mulighetsrommet-api' node ./server/main.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/ --fix",
    "typegen": "react-router typegen",
    "typecheck": "tsc --noEmit",
    "test": "vitest",
    "build": "react-router typegen && react-router build",
    "build:demo": "cross-env VITE_MULIGHETSROMMET_API_MOCK='true' DISABLE_DEKORATOR='true' react-router build",
    "pack": "pnpm --filter @mr/arrangor-flate --prod deploy --legacy dist",
    "pack:demo": "pnpm --filter @mr/arrangor-flate --prod deploy --legacy dist",
    "playwright": "cross-env DISABLE_DEKORATOR='true' playwright test ",
    "playwright:debug": "cross-env DISABLE_DEKORATOR='true' playwright test --debug",
    "playwright:open": "cross-env DISABLE_DEKORATOR='true' playwright test --ui",
    "playwright:codegen": "playwright codegen playwright.dev",
    "api-client:build": "openapi-ts"
  },
  "dependencies": {
    "@grafana/faro-react": "2.2.0",
    "@mjackson/form-data-parser": "0.9.1",
    "@mr/frontend-common": "workspace:*",
    "@navikt/aksel-icons": "8.2.2",
    "@navikt/ds-css": "8.2.2",
    "@navikt/ds-react": "8.2.2",
    "@navikt/ds-tailwind": "8.2.2",
    "@navikt/nav-dekoratoren-moduler": "3.6.2",
    "@navikt/oasis": "4.1.2",
    "@react-router/express": "7.13.0",
    "@react-router/node": "7.13.0",
    "@react-router/serve": "7.13.0",
    "@tanstack/react-query": "5.90.20",
    "compression": "1.8.1",
    "csp-header": "6.3.0",
    "express": "5.2.1",
    "express-prom-bundle": "8.0.0",
    "html-react-parser": "5.2.16",
    "http-proxy-middleware": "3.0.5",
    "isbot": "5.1.34",
    "morgan": "1.10.1",
    "prom-client": "15.1.3",
    "react": "19.0.3",
    "react-dom": "19.0.3",
    "react-router": "7.13.0",
    "msw": "2.12.7",
    "winston": "3.19.0",
    "cross-env": "10.1.0",
    "uuid": "13.0.0"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.11.0",
    "@eslint/js": "9.39.2",
    "@hey-api/openapi-ts": "0.90.10",
    "@playwright/test": "1.58.0",
    "@react-router/dev": "7.13.0",
    "@react-router/fs-routes": "7.13.0",
    "@tailwindcss/postcss": "4.1.18",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "4.1.18",
    "@types/compression": "1.8.1",
    "@types/express": "5.0.6",
    "@types/morgan": "1.9.10",
    "@types/node": "25.2.0",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "autoprefixer": "10.4.24",
    "eslint": "9.39.2",
    "eslint-plugin-prettier": "5.5.5",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "globals": "17.3.0",
    "jsdom": "27.4.0",
    "playwright": "1.58.0",
    "postcss": "8.5.6",
    "postcss-import": "16.1.1",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vite": "7.3.1",
    "vite-tsconfig-paths": "6.0.5",
    "vitest": "4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  },
