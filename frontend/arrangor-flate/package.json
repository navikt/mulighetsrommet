{
  "name": "@mr/arrangor-flate",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_MULIGHETSROMMET_API_MOCK='true' node ./server/main.js",
    "dev:vite": "react-router dev",
    "backend": "cross-env VITE_MULIGHETSROMMET_API_BASE='http://localhost:8080' node ./server/main.js",
    "start": "cross-env NODE_ENV=production VITE_MULIGHETSROMMET_API_BASE='http://mulighetsrommet-api' node ./server/main.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/ --fix",
    "typecheck": "react-router typegen && tsc",
    "build": "react-router build",
    "pack": "pnpm --filter @mr/arrangor-flate --prod deploy dist",
    "playwright": "playwright test",
    "playwright:debug": "playwright test --debug",
    "playwright:open": "playwright test --ui",
    "playwright:codegen": "playwright codegen playwright.dev"
  },
  "dependencies": {
    "@axe-core/playwright": "4.10.1",
    "@mr/api-client": "workspace:*",
    "@mr/frontend-common": "workspace:*",
    "@navikt/aksel-icons": "7.9.0",
    "@navikt/ds-css": "7.9.0",
    "@navikt/ds-react": "7.9.0",
    "@navikt/ds-tailwind": "7.9.0",
    "@navikt/nav-dekoratoren-moduler": "3.1.3",
    "@navikt/oasis": "3.6.0",
    "@react-router/express": "7.1.1",
    "@react-router/node": "7.1.1",
    "@react-router/serve": "7.1.1",
    "compression": "1.7.5",
    "cross-env": "7.0.3",
    "csp-header": "6.0.0",
    "express": "4.21.2",
    "express-prom-bundle": "8.0.0",
    "html-react-parser": "5.2.2",
    "http-proxy-middleware": "3.0.3",
    "isbot": "5.1.19",
    "jsdom": "25.0.1",
    "morgan": "1.10.0",
    "msw": "2.7.0",
    "prom-client": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.1.1",
    "uuid": "11.0.4",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@playwright/test": "1.49.1",
    "@react-router/dev": "7.1.1",
    "@react-router/fs-routes": "7.1.1",
    "@tailwindcss/typography": "0.5.15",
    "@types/compression": "1.7.5",
    "@types/express": "5.0.0",
    "@types/morgan": "1.9.9",
    "@types/node": "22.10.5",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.2",
    "@typescript-eslint/eslint-plugin": "8.19.0",
    "@typescript-eslint/parser": "8.19.0",
    "autoprefixer": "10.4.20",
    "eslint": "9.17.0",
    "eslint-plugin-react": "7.37.3",
    "eslint-plugin-react-hooks": "5.1.0",
    "globals": "15.14.0",
    "playwright": "1.49.1",
    "postcss": "8.4.49",
    "postcss-import": "16.1.0",
    "tailwindcss": "3.4.17",
    "typescript": "5.7.2",
    "vite": "6.0.7",
    "vite-tsconfig-paths": "5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "files": [
    "build",
    "server"
  ]
