{
  "name": "@mr/arrangor-flate",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "cross-env VITE_MULIGHETSROMMET_API_MOCK='true' node ./server/main.js",
    "dev:vite": "react-router dev",
    "backend": "cross-env VITE_MULIGHETSROMMET_API_BASE='http://localhost:8080' node ./server/main.js",
    "start": "cross-env NODE_ENV=production VITE_MULIGHETSROMMET_API_BASE='http://mulighetsrommet-api' node ./server/main.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/",
    "lint:fix": "eslint --cache --cache-location ./node_modules/.cache/eslint app/ server/ --fix",
    "typecheck": "react-router typegen && tsc",
    "build": "react-router build",
    "pack": "pnpm --filter @mr/arrangor-flate --prod deploy dist",
    "playwright": "cross-env DISABLE_DEKORATOR='true' playwright test ",
    "playwright:debug": "playwright test --debug",
    "playwright:open": "playwright test --ui",
    "playwright:codegen": "playwright codegen playwright.dev",
    "api-client:build": "openapi-ts --useOptions --input ../../mulighetsrommet-api/src/main/resources/web/openapi.yaml --output ./api-client"
  },
  "dependencies": {
    "@axe-core/playwright": "4.10.1",
    "@hey-api/client-fetch": "0.10.2",
    "@mjackson/file-storage": "^0.6.1",
    "@mjackson/form-data-parser": "^0.7.0",
    "@mr/frontend-common": "workspace:*",
    "@navikt/aksel-icons": "7.21.1",
    "@navikt/ds-css": "7.21.1",
    "@navikt/ds-react": "7.21.1",
    "@navikt/ds-tailwind": "7.21.1",
    "@navikt/nav-dekoratoren-moduler": "3.2.3",
    "@navikt/oasis": "3.8.0",
    "@react-router/express": "7.6.1",
    "@react-router/node": "7.6.1",
    "@react-router/serve": "7.6.1",
    "@tailwindcss/postcss": "4.1.7",
    "@tailwindcss/vite": "4.1.7",
    "compression": "1.8.0",
    "cross-env": "7.0.3",
    "csp-header": "6.1.0",
    "express": "5.1.0",
    "express-prom-bundle": "8.0.0",
    "html-react-parser": "5.2.5",
    "http-proxy-middleware": "3.0.5",
    "isbot": "5.1.28",
    "jsdom": "26.1.0",
    "morgan": "1.10.0",
    "msw": "2.8.4",
    "prom-client": "15.1.3",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-router": "7.6.1",
    "uuid": "11.1.0",
    "winston": "3.17.0"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "0.67.6",
    "@playwright/test": "1.52.0",
    "@react-router/dev": "7.6.1",
    "@react-router/fs-routes": "7.6.1",
    "@tailwindcss/typography": "0.5.16",
    "@types/compression": "1.8.0",
    "@types/express": "5.0.2",
    "@types/morgan": "1.9.9",
    "@types/node": "22.15.21",
    "@types/react": "19.0.3",
    "@types/react-dom": "19.0.4",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "autoprefixer": "10.4.21",
    "eslint": "9.27.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "globals": "16.2.0",
    "playwright": "1.52.0",
    "postcss": "8.5.3",
    "postcss-import": "16.1.0",
    "tailwindcss": "4.1.7",
    "typescript": "5.8.3",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "files": [
    "build",
    "server"
  ]
}
