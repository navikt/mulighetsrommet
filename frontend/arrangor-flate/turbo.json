{
  "extends": ["//"],
  "tasks": {
    "api-client:build": {
      "outputs": ["api-client"]
    },
    "backend": {
      "dependsOn": ["api-client:build"],
      "outputs": []
    },
    "dev": {
      "dependsOn": ["api-client:build"],
      "outputs": []
    },
    "lint:fix": {
      "dependsOn": ["api-client:build"],
      "outputs": []
    },
    "react-router:typegen": {
      "outputs": [".react-router"]
    },
    "typecheck": {
      "dependsOn": ["api-client:build", "react-router:typegen"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["api-client:build", "react-router:typegen"],
      "outputs": ["build"]
    },
    "build:demo": {
      "dependsOn": ["api-client:build", "react-router:typegen"],
      "outputs": ["build"]
    },
    "pack": {
      "env": ["NODE_AUTH_TOKEN", "NPM_CONFIG_USERCONFIG"],
      "dependsOn": ["build"],
      "outputs": ["dist"]
    },
    "pack:demo": {
      "env": ["NODE_AUTH_TOKEN", "NPM_CONFIG_USERCONFIG"],
      "dependsOn": ["build:demo"],
      "outputs": ["dist"]
    },
    "playwright": {
      "dependsOn": ["api-client:build"],
      "outputs": []
    }
  }
}
