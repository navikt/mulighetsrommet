    "clean": "rimraf build",
    "cy:run": "npx cypress run --e2e",
    "cypress": "npx cypress open",
    "lint": "eslint src --ext .js,.ts,.tsx",
    "prettier": "prettier --write 'src/**/*.ts{,x}'",
    "serve": "vite preview",
    "start": "cross-env VITE_ENVIRONMENT=localhost VITE_MULIGHETSROMMET_API_MOCK=true VITE_EKSTERNE_SYSTEMER_MOCK=true vite"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
