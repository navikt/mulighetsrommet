{
  "name": "mulighetsrommet-veileder-flate",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "backend": "cross-env VITE_MULIGHETSROMMET_API_MOCK=false VITE_MULIGHETSROMMET_API_BASE='http://localhost:8080' vite",
    "build": "cross-env VITE_ENVIRONMENT=production VITE_FARO_URL=https://telemetry.nav.no/collect VITE_MULIGHETSROMMET_API_BASE='/mulighetsrommet-api' vite build --base=https://mulighetsrommet-veileder-flate.intern.nav.no/",
    "build:dev": "cross-env VITE_ENVIRONMENT=dev VITE_FARO_URL=https://telemetry.ekstern.dev.nav.no/collect VITE_MULIGHETSROMMET_API_BASE='/mulighetsrommet-api' vite build --base=https://mulighetsrommet-veileder-flate.intern.dev.nav.no/",
    "build:demo": "cross-env VITE_DEMO_FNR=12345678910 VITE_DEMO_ENHET=0315 VITE_MULIGHETSROMMET_API_MOCK=true vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0 && tsc",
    "fix-lint": "eslint src --ext .ts,.tsx --fix",
    "preview": "vite preview",
    "start": "cross-env VITE_ENVIRONMENT=localhost VITE_MULIGHETSROMMET_API_MOCK=true vite",
    "playwright": "playwright test",
    "playwright:debug": "playwright test --debug",
    "playwright:open": "playwright test --ui",
    "playwright:codegen": "playwright codegen playwright.dev"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie < 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@grafana/faro-web-sdk": "1.3.5",
    "@navikt/aksel-icons": "5.12.2",
    "@navikt/ds-css": "5.12.2",
    "@navikt/ds-react": "5.12.2",
    "@navikt/navspa": "6.0.1",
    "@portabletext/react": "3.0.11",
    "@tanstack/react-query": "5.17.0",
    "@tanstack/react-query-devtools": "5.17.0",
    "axe-core": "4.8.3",
    "classnames": "2.5.1",
    "jotai": "2.6.0",
    "jotai-location": "0.5.2",
    "mulighetsrommet-api-client": "*",
    "mulighetsrommet-frontend-common": "*",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
