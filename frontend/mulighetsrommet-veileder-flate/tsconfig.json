{
  "compilerOptions": {
    "module": "esnext",
    "target": "es6",
    "lib": ["dom", "DOM.Iterable", "ESNext", "es5", "es6", "es7", "es2016.array.include", "es2017"],
    "allowJs": false,
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "esModuleInterop": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictPropertyInitialization": false,
    "noFallthroughCasesInSwitch": true,
    "baseUrl": "./",
    "types": ["cypress", "cypress-axe", "vite/client", "vite-plugin-svgr/client"]
  },
  "exclude": ["node_modules", "build", "scripts", "acceptance-tests", "webpack", "src/setupTests.ts"],
  "include": ["src", "mock"]
}
