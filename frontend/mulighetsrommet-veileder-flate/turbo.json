{
  "extends": ["//"],
  "tasks": {
    "build:api-client": {
      "outputs": ["api-client"]
    },
    "dev": {
      "dependsOn": ["build:api-client"],
      "outputs": []
    },
    "backend": {
      "dependsOn": ["build:api-client"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["build:api-client"],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": ["build:api-client"],
      "outputs": []
    },
    "playwright": {
      "dependsOn": ["build:api-client"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["build:api-client"],
      "outputs": ["dist"]
    },
    "build:*": {
      "dependsOn": ["build:api-client", "^build"],
      "outputs": ["dist"]
    }
  }
}
