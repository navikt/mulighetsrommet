{{#if value}}
    {{#if format}}
        {{#eq format "DATE"}}
            {{iso_to_nor_date value}}
        {{/eq}}
        {{#eq format "PERCENT"}}
            {{value}} %
        {{/eq}}
        {{#eq format "STATUS_SUCCESS"}}
            <span class="status_success">{{value}}</span>
        {{/eq}}
    {{else if currency}}
        {{currency_no value}} {{currency}}
    {{else}}
        {{value}}
    {{/if}}
{{else}}
    -
{{/if}}
