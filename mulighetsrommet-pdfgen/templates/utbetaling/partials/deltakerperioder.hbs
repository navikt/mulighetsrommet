                <td class="text-align-right">{{iso_to_nor_date periode.slutt}}</td>
                <td class="text-align-right">{{deltakelsesprosent}}%</td>
            </tr>
            {{/each}}
            {{/each}}
        </tbody>
    </table>
    {{/if}}
</div>