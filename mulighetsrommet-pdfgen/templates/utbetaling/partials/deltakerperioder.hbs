<div class="block">
    <h4>Deltakerperioder</h4>
    {{#if utbetaling.beregning.deltakelser.length}}
    <table class="deltaker-liste">
        <thead>
            <tr>
                <th>Navn</th>
                <th class="text-align-right">FÃ¸dselsdato</th>
                <th class="text-align-right">Startdato i perioden</th>
                <th class="text-align-right">Sluttdato i perioden</th>
                <th class="text-align-right">Deltakelsesprosent</th>
            </tr>
        </thead>
        <tbody>
            {{#each utbetaling.beregning.deltakelser}}
            {{#each perioder}}
            <tr>
