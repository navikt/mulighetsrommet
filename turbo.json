{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/dev",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"]
    },
    "build:labs": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "mulighetsrommet-cms#deploy": {
      "dependsOn": ["build", "$SANITY_AUTH_TOKEN"]
    },
    "dev": {
      "cache": false
    }
  }
}
