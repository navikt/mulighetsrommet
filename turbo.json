{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build",
        "$VITE_MULIGHETSROMMET_API_MOCK",
        "$VITE_MULIGHETSROMMET_API_BASE"
      ]
    },
    "mulighetsrommet-api-client#build": {
      "inputs": ["mulighetsrommet-api/src/main/resources/web/openapi.yml"]
    },
    "build:mock": {
      "dependsOn": ["^build"]
    },
    "build:preprod": {
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": []
    },
    "mulighetsrommet-cms#deploy": {
      "dependsOn": ["build", "$SANITY_AUTH_TOKEN"]
    },
    "dev": {
      "cache": false
    }
  }
}
