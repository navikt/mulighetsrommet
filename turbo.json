{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:mock": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:labs": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:preprod": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "mulighetsrommet-api-client#build": {
      "inputs": ["../../mulighetsrommet-api/src/main/resources/web/openapi.yaml"],
      "outputs": ["build"]
    },
    "norg2-api-client#build": {
      "inputs": ["openapi-norg2.json"],
      "outputs": ["build"]
    },
    "dev": {
      "cache": false
    }
  }
}
