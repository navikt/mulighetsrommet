{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": [
    "mulighetsrommet-api/src/main/resources/web/openapi.yaml"
  ],
  "tasks": {
    "fix-lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:demo": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:lokal": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "playwright": {
      "env": ["PLAYWRIGHT_BROWSERS_PATH"],
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
