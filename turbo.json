{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "lint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "test:ci": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:mock": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:labs": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "build:preprod": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
